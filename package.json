{
  "name": "cyezoi-food-system",
  "version": "2.0.0",
  "description": "曹杨二中周边美食评分系统",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev --prefix frontend\" \"nodemon server.js\"",
    "start": "npm run build:frontend && concurrently \"npm run preview --prefix frontend\" \"node server.js\"",
    "build:frontend": "npm run build --prefix frontend",
    "preview:frontend": "npm run preview --prefix frontend",
    "create-admin": "node scripts/createAdmin.js",
    "seed-data": "node scripts/seedData.js",
    "seed-reviews": "node scripts/seedReviews.js",
    "test-db": "node scripts/testConnection.js",
    "diagnose": "node scripts/diagnose.js"
  },
  "keywords": [
    "food",
    "rating",
    "api",
    "user-system"
  ],
  "author": "CYEZOI",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}